@if (Model._ProfileSection != null) {
    foreach (var item in Model.ProfileSectionDistinct) {
        <section id="questions" class="upcoming-wedding-main-block profile_section" style="background-color: #faf9f5;">
            <div class="container">
                <div class="row">
                    <hr>
                    <h3 class="vendor-profile-listing-heading" id="@item.Name">@item.Name</h3>
                    @foreach (var profileSection in Model.ProfileSection.Where(x => x.SectionHeadId == item.Id)) {

                        if (profileSection.SectionContent.ContentTypeId == 1)//text
                        {
                            <div class="col-md-4" style="padding: 5px;">
                                <div class="panel panel-default" style="min-height: 100%; height: 100%; margin-bottom: 0; padding: 5px;">
                                    <div class="panel-heading" role="tab" id="headingFour">
                                        <h6 class="panel-title question-heading">
                                            <i style="color: #ff1783;" class="fa fa-check"></i>&nbsp;
                                            @profileSection.SectionContent.Content
                                        </h6>
                                    </div>
                                </div>
                            </div>
                        }
                        else if (profileSection.SectionContent.ContentTypeId == 2) //images
                        {
                            <div class="col-lg-3 col-md-3 col-sm-3" style=" padding: 0; ">
                                <div class="gallery-block" style="border: 5px solid transparent;">
                                    <div class="thumbnail" style="height: 200px; padding: 0; margin-bottom: 0; border-radius: 0;">
